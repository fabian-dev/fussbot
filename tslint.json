{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": [
      false
    ],
    "no-namespace": [
      "allow-declarations"
    ],
    "interface-name": ["never-prefix"],
    "no-trailing-whitespace": [
      "ignore-comments"
    ],
    "no-var-requires": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-consecutive-blank-lines": false
  },
  "rulesDirectory": []
}